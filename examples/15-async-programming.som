ҳамзамон функсия маълумот_гирифтан(url: сатр): сатр {
    чоп.сабт("Дархости маълумот аз: " + url);
    
    // Simulate async operation
    тағйирёбанда натиҷа = "Маълумот аз " + url;
    бозгашт натиҷа;
}

ҳамзамон функсия коркарди_маълумот(): холӣ {
    чоп.сабт("Оғози коркарди маълумот...");
    
    тағйирёбанда маълумот1 = интизор маълумот_гирифтан("/api/users");
    чоп.сабт("Гирифта шуд: " + маълумот1);
    
    тағйирёбанда маълумот2 = интизор маълумот_гирифтан("/api/posts");
    чоп.сабт("Гирифта шуд: " + маълумот2);
    
    чоп.сабт("Ҳамаи маълумот коркард шуд");
}

