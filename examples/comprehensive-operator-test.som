// Complete TypeScript operator test suite

функсия тести_пурраи_операторҳо(): холӣ {
    чоп.сабт("=== Санҷиши пурраи ҳамаи операторҳои TypeScript ===\n");
    
    // Arithmetic operators
    чоп.сабт("1. Операторҳои арифметикӣ:");
    тағйирёбанда а = 12;
    тағйирёбанда б = 5;
    чоп.сабт("а + б = " + (а + б));     // 17
    чоп.сабт("а - б = " + (а - б));     // 7
    чоп.сабт("а * б = " + (а * б));     // 60
    чоп.сабт("а / б = " + (а / б));     // 2.4
    чоп.сабт("а % б = " + (а % б));     // 2
    
    // Assignment operators
    чоп.сабт("\n2. Операторҳои таъинот:");
    тағйирёбанда в = 20;
    в += 5; чоп.сабт("в += 5: " + в);   // 25
    в -= 3; чоп.сабт("в -= 3: " + в);   // 22
    в *= 2; чоп.сабт("в *= 2: " + в);   // 44
    в /= 4; чоп.сабт("в /= 4: " + в);   // 11
    в %= 3; чоп.сабт("в %= 3: " + в);   // 2
    
    // Comparison operators
    чоп.сабт("\n3. Операторҳои муқоиса:");
    тағйирёбанда х = 10;
    тағйирёбанда у = 15;
    чоп.сабт("х == у: " + (х == у));    // false
    чоп.сабт("х != у: " + (х != у));    // true
    чоп.сабт("х < у: " + (х < у));      // true
    чоп.сабт("х > у: " + (х > у));      // false
    чоп.сабт("х <= у: " + (х <= у));    // true
    чоп.сабт("х >= у: " + (х >= у));    // false
    
    // Logical operators
    чоп.сабт("\n4. Операторҳои мантиқӣ:");
    тағйирёбанда дуруст_қимат = дуруст;
    тағйирёбанда нодуруст_қимат = нодуруст;
    чоп.сабт("дуруст && нодуруст: " + (дуруст_қимат && нодуруст_қимат)); // false
    чоп.сабт("дуруст || нодуруст: " + (дуруст_қимат || нодуруст_қимат)); // true
    чоп.сабт("!дуруст: " + (!дуруст_қимат));                              // false
    
    // Bitwise operators
    чоп.сабт("\n5. Операторҳои битӣ:");
    тағйирёбанда н1 = 12;  // 1100
    тағйирёбанда н2 = 10;  // 1010
    чоп.сабт("12 & 10 = " + (н1 & н2));    // 8 (1000)
    чоп.сабт("12 | 10 = " + (н1 | н2));    // 14 (1110)
    чоп.сабт("12 ^ 10 = " + (н1 ^ н2));    // 6 (0110)
    чоп.сабт("~12 = " + (~н1));             // -13
    
    // Shift operators
    чоп.сабт("\n6. Операторҳои интиқол:");
    чоп.сабт("12 << 2 = " + (н1 << 2));    // 48
    чоп.сабт("12 >> 2 = " + (н1 >> 2));    // 3
    
    // Bitwise assignment operators
    чоп.сабт("\n7. Операторҳои таъиноти битӣ:");
    тағйирёбанда г = 15;   // 1111
    г &= 7;  чоп.сабт("15 &= 7: " + г);    // 7 (0111)
    г |= 8;  чоп.сабт("7 |= 8: " + г);     // 15 (1111)
    г ^= 3;  чоп.сабт("15 ^= 3: " + г);    // 12 (1100)
    
    // Increment/Decrement
    чоп.сабт("\n8. Операторҳои афзоиш/кам:");
    тағйирёбанда шумора = 10;
    шумора++; чоп.сабт("шумора++: " + шумора); // 11
    шумора--; чоп.сабт("шумора--: " + шумора); // 10
    
    // Ternary operator
    чоп.сабт("\n9. Оператори тернарӣ:");
    тағйирёбанда макс = а > б ? а : б;
    чоп.сабт("макс = а > б ? а : б = " + макс);
    
    чоп.сабт("\n=== Ҳамаи операторҳо бо муваффақият санҷида шуданд! ===");
}

тести_пурраи_операторҳо();
