// Test advanced TypeScript operators

функсия тест_пешрафта_операторҳо(): холӣ {
    чоп.сабт("=== Санҷиши операторҳои пешрафта ===");
    
    // Test modulo assignment
    тағйирёбанда а = 17;
    чоп.сабт("а = 17");
    а %= 5;
    чоп.сабт("а %= 5 равно:");
    чоп.сабт(а);
    
    // Test increment/decrement
    тағйирёбанда счётчик = 10;
    чоп.сабт("счётчик = 10");
    
    счётчик++;
    чоп.сабт("счётчик++ равно:");
    чоп.сабт(счётчик);
    
    счётчик--;
    чоп.сабт("счётчик-- равно:");
    чоп.сабт(счётчик);
    
    // Test logical operators
    тағйирёбанда дуруст_қимат = дуруст;
    тағйирёбанда нодуруст_қимат = нодуруст;
    
    чоп.сабт("дуруст && нодуруст:");
    чоп.сабт(дуруст_қимат && нодуруст_қимат);
    
    чоп.сабт("дуруст || нодуруст:");
    чоп.сабт(дуруст_қимат || нодуруст_қимат);
    
    чоп.сабт("!дуруст:");
    чоп.сабт(!дуруст_қимат);
    
    // Test ternary operator
    тағйирёбанда х = 5;
    тағйирёбанда у = 10;
    тағйирёбанда макс = х > у ? х : у;
    чоп.сабт("макс = х > у ? х : у:");
    чоп.сабт(макс);
}

тест_пешрафта_операторҳо();
