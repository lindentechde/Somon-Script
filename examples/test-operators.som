// Test all TypeScript operators in SomonScript

функсия санҷиши_операторҳо(): холӣ {
    чоп.сабт("=== Санҷиши ҳамаи операторҳо ===\n");
    
    // Arithmetic operators
    чоп.сабт("--- Операторҳои арифметикӣ ---");
    тағйирёбанда а: рақам = 10;
    тағйирёбанда б: рақам = 3;
    
    чоп.сабт("а = " + а + ", б = " + б);
    чоп.сабт("а + б = " + (а + б));
    чоп.сабт("а - б = " + (а - б));
    чоп.сабт("а * б = " + (а * б));
    чоп.сабт("а / б = " + (а / б));
    чоп.сабт("а % б = " + (а % б));
    
    // Test exponentiation (if supported)
    // чоп.сабт("а ** б = " + (а ** б));
    
    // Assignment operators
    чоп.сабт("\n--- Операторҳои таъинот ---");
    тағйирёбанда в: рақам = 15;
    чоп.сабт("в = " + в);
    
    в += 5;
    чоп.сабт("в += 5: " + в);
    
    в -= 3;
    чоп.сабт("в -= 3: " + в);
    
    в *= 2;
    чоп.сабт("в *= 2: " + в);
    
    в /= 4;
    чоп.сабт("в /= 4: " + в);
    
    в %= 3;
    чоп.сабт("в %= 3: " + в);
    
    // Comparison operators
    чоп.сабт("\n--- Операторҳои муқоиса ---");
    тағйирёбанда х: рақам = 5;
    тағйирёбанда у: рақам = 5;
    тағйирёбанда з: рақам = 10;
    
    чоп.сабт("х = " + х + ", у = " + у + ", з = " + з);
    чоп.сабт("х == у: " + (х == у));
    чоп.сабт("х != з: " + (х != з));
    чоп.сабт("х < з: " + (х < з));
    чоп.сабт("х <= у: " + (х <= у));
    чоп.сабт("з > х: " + (з > х));
    чоп.сабт("з >= у: " + (з >= у));
    
    // Logical operators
    чоп.сабт("\n--- Операторҳои мантиқӣ ---");
    тағйирёбанда дуруст1: мантиқӣ = дуруст;
    тағйирёбанда дуруст2: мантиқӣ = дуруст;
    тағйирёбанда нодуруст1: мантиқӣ = нодуруст;
    
    чоп.сабт("дуруст1 && дуруст2: " + (дуруст1 && дуруст2));
    чоп.сабт("дуруст1 && нодуруст1: " + (дуруст1 && нодуруст1));
    чоп.сабт("дуруст1 || нодуруст1: " + (дуруст1 || нодуруст1));
    чоп.сабт("!дуруст1: " + (!дуруст1));
    чоп.сабт("!нодуруст1: " + (!нодуруст1));
    
    // Increment/Decrement
    чоп.сабт("\n--- Операторҳои афзоиш/кам ---");
    тағйирёбанда шумора: рақам = 10;
    чоп.сабт("шумора = " + шумора);
    
    шумора++;
    чоп.сабт("шумора++: " + шумора);
    
    шумора--;
    чоп.сабт("шумора--: " + шумора);
    
    // Ternary operator
    чоп.сабт("\n--- Оператори тернарӣ ---");
    тағйирёбанда калон: рақам = а > б ? а : б;
    чоп.сабт("калон = а > б ? а : б = " + калон);
    
    чоп.сабт("\n=== Санҷиш анҷом ёфт! ===");
}

// Run the test
санҷиши_операторҳо();
