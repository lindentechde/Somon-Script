// Simple arithmetic test

функсия тест_арифметика(): холӣ {
    тағйирёбанда а = 10;
    тағйирёбанда б = 3;
    
    чоп.сабт("а равно:");
    чоп.сабт(а);
    чоп.сабт("б равно:");
    чоп.сабт(б);
    
    тағйирёбанда ҷамъ = а + б;
    чоп.сабт("ҷамъ равно:");
    чоп.сабт(ҷамъ);
    
    тағйирёбанда тафрика = а - б;
    чоп.сабт("тафрика равно:");
    чоп.сабт(тафрика);
}

тест_арифметика();
