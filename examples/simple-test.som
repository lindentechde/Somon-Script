// Simple arithmetic test with Type Annotations

функсия тест_арифметика(): холӣ {
    тағйирёбанда а: рақам = 10;
    тағйирёбанда б: рақам = 3;
    
    чоп.сабт("а равно:");
    чоп.сабт(а);
    чоп.сабт("б равно:");
    чоп.сабт(б);
    
    тағйирёбанда ҷамъ: рақам = а + б;
    чоп.сабт("ҷамъ равно:");
    чоп.сабт(ҷамъ);
    
    тағйирёбанда тафрика: рақам = а - б;
    чоп.сабт("тафрика равно:");
    чоп.сабт(тафрика);
}

тест_арифметика();
