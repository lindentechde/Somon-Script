// Built-in Objects and Methods (Объектҳо ва методҳои дарунсохт) Example
чоп.сабт("=== Объектҳо ва Методҳои Дарунсохт ===");

// Object methods
чоп.сабт("\n=== Методҳои Объект ===");

тағйирёбанда корбар = {
    ном: "Раҳмон",
    синни_сол: 28,
    шаҳр: "Душанбе",
    касб: "Барномасоз"
};

// Object.keys() - калидҳо
тағйирёбанда калидҳо = объект.калидҳо(корбар);
чоп.сабт("Калидҳои объект: " + калидҳо.join(", "));

// Object.values() - қиматҳо
тағйирёбанда қиматҳо = объект.қиматҳо(корбар);
чоп.сабт("Қиматҳои объект: " + қиматҳо.join(", "));

// Object.entries()
тағйирёбанда вурудҳо = объект.entries(корбар);
барои (тағйирёбанда вуруд аз вурудҳо) {
    чоп.сабт("  " + вуруд[0] + ": " + вуруд[1]);
}

// Object.assign() - copying and merging
тағйирёбанда нусха = объект.assign({}, корбар);
чоп.сабт("Нусхаи объект: " + объект.калидҳо(нусха).дарозӣ + " хосиятҳо");

тағйирёбанда маълумоти_иловагӣ = {
    почта: "rahmon@example.com",
    телефон: "+992 900 123456"
};

тағйирёбанда корбари_пурра = объект.assign({}, корбар, маълумоти_иловагӣ);
чоп.сабт("Корбари пурра: " + объект.калидҳо(корбари_пурра).дарозӣ + " хосиятҳо");

// Object.freeze() and Object.seal()
тағйирёбанда собит_объект = объект.freeze({ рақам: 42, ном: "Собит" });
// собит_объект.рақам = 100; // This would fail - object is frozen

тағйирёбанда муҳршуда = объект.seal({ калид: "қимат" });
муҳршуда.калид = "қимати_нав"; // OK - can modify existing
// муҳршуда.нав_калид = "test"; // Would fail - can't add new properties

// Math operations
чоп.сабт("\n=== Амалиёти Математикӣ ===");

// Basic math operations
чоп.сабт("PI: " + математика.PI);
чоп.сабт("E: " + математика.E);

// Math methods
тағйирёбанда рақам = -15.7;
чоп.сабт("\nБарои рақам " + рақам + ":");
чоп.сабт("Мутлақ: " + математика.abs(рақам));
чоп.сабт("Даврзанӣ: " + математика.round(15.7));
чоп.сабт("Боло: " + математика.ceil(15.1));
чоп.сабт("Поён: " + математика.floor(15.9));

// Power and roots
чоп.сабт("\nДараҷа ва решаҳо:");
чоп.сабт("2^8: " + математика.pow(2, 8));
чоп.сабт("Решаи квадратии 64: " + математика.sqrt(64));
чоп.сабт("Решаи кубии 27: " + математика.cbrt(27));

// Trigonometric functions
чоп.сабт("\nФунксияҳои тригонометрӣ:");
чоп.сабт("sin(PI/2): " + математика.sin(математика.PI / 2));
чоп.сабт("cos(PI): " + математика.cos(математика.PI));
чоп.сабт("tan(PI/4): " + математика.tan(математика.PI / 4));

// Min, max, random
чоп.сабт("\nМин, Макс, Тасодуфӣ:");
тағйирёбанда рақамҳо = [45, 12, 78, 23, 56, 89, 34];
чоп.сабт("Рақамҳо: " + рақамҳо.join(", "));
чоп.сабт("Максимум: " + математика.max(...рақамҳо));
чоп.сабт("Минимум: " + математика.min(...рақамҳо));
чоп.сабт("Рақами тасодуфӣ (0-1): " + математика.random());
чоп.сабт("Рақами тасодуфӣ (1-100): " + математика.floor(математика.random() * 100) + 1);

// String methods in detail
чоп.сабт("\n=== Методҳои Пурраи Сатр ===");

тағйирёбанда матн = "  Забони барномасозии Сомон  ";

чоп.сабт("Матни аслӣ: '" + матн + "'");
чоп.сабт("Дарозӣ: " + матн.дарозӣ);
чоп.сабт("Trim: '" + матн.trim() + "'");
чоп.сабт("Калонҳарф: " + матн.toUpperCase());
чоп.сабт("Хурдҳарф: " + матн.toLowerCase());

тағйирёбанда калима = "Сомон";
чоп.сабт("\nКор бо '" + калима + "':");
чоп.сабт("Мавқеи 'о': " + калима.indexOf('о'));
чоп.сабт("Мавқеи охирини 'о': " + калима.lastIndexOf('о'));
чоп.сабт("Дорад 'мон': " + калима.includes("мон"));
чоп.сабт("Сар мешавад бо 'Со': " + калима.startsWith("Со"));
чоп.сабт("Хотима меёбад бо 'он': " + калима.endsWith("он"));

// String replacement and splitting
тағйирёбанда ҷумла = "Ман дар Тоҷикистон зиндагӣ мекунам";
чоп.сабт("\nҶойивазкунӣ:");
чоп.сабт(ҷумла.ҷойивазкунӣ("Тоҷикистон", "Душанбе"));

тағйирёбанда калимаҳо = ҷумла.split(" ");
чоп.сабт("Калимаҳо: " + калимаҳо.дарозӣ + " - " + калимаҳо.join(" | "));

// Substring operations
тағйирёбанда матни_дароз = "БарномасозииТоҷикӣ";
чоп.сабт("\nЗерсатрҳо:");
чоп.сабт("substring(0, 11): " + матни_дароз.substring(0, 11));
чоп.сабт("substr(11, 6): " + матни_дароз.substr(11, 6));
чоп.сабт("slice(11): " + матни_дароз.slice(11));
чоп.сабт("charAt(5): " + матни_дароз.charAt(5));
чоп.сабт("charCodeAt(0): " + матни_дароз.charCodeAt(0));

