// Array manipulation
чоп.сабт("\nТағйири массив:");
тағйирёбанда массив = [1, 2, 3];
массив.илова(4); // push
чоп.сабт("Пас аз илова: " + массив.join(", "));

массив.unshift(0); // add to beginning
чоп.сабт("Пас аз unshift: " + массив.join(", "));

тағйирёбанда охирин = массив.баровардан(); // pop
чоп.сабт("Баровардани охирин (" + охирин + "): " + массив.join(", "));

тағйирёбанда аввалин = массив.shift();
чоп.сабт("Shift аввалин (" + аввалин + "): " + массив.join(", "));
