чоп.сабт("Рақамро ворид кунед:");
тағйирёбанда дод = 10;

функсия Факториал(n: рақам): рақам {
    тағйирёбанда факториал = 1;
    барои (тағйирёбанда i = 1; i <= n; i++) {
        факториал = факториал * i;
    }
    бозгашт факториал;
}

тағ натиҷа = Факториал(дод);

чоп.сабт("натиҷаи " + дод + " != " + натиҷа);